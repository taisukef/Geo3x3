関数: 切り捨て(実数: 数) => (整数: 結果) {
	整数: 整数部 = 数;
	実数: 小数部 = 数 - 整数部;
	結果 = 数 - 小数部;
}

関数: エンコード(実数: 緯度, 実数: 経度, 整数: レベル) => (文字列: Geo3x3) {
  Geo3x3 = "E";
  もし (経度 < 0.0) ならば {
    Geo3x3 = "W";
    経度 += 180.0;
  }
  緯度 += 90.0;
  実数: 単位 = 180.0;
  (整数: カウンタ = 1;) から (カウンタ == レベル) まで (カウンタ++;) {
    単位 /= 3.0;
    整数: 横 = 切り捨て(経度 / 単位);
    整数: 縦 = 切り捨て(緯度 / 単位);
    Geo3x3 += 横 + 縦 * 3 + 1;
    経度 -= 横 * 単位;
    緯度 -= 縦 * 単位;
  }
}
関数: 実行() => (){
	表示(エンコード(35.65858, 139.745433, 14));
}
